#!/usr/bin/env bash
# This script will give information about a particular file.
if [[ -e school ]]
then
    echo "school file exists"
    if [ find school -empty ]
    then
	echo "school file is empty"
    else
	echo "school file is not empty"
    fi
    if [ -f school ]
    then
	echo "school is a regular file"
    fi
else
    echo "school file does not exist"
fi
